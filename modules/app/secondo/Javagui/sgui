if [ "$SECONDO_PLATFORM" = "win32" ]
    then S=";"
    else S=":"
fi    

FEF="-Dfile.encoding=UTF-8"

CP="lib/java_cup_v10k_runtime.jar"$S"lib/jl1.0.1.jar"$S"."$S"lib/pdfbox-app-2.0.8.jar""$S"lib/je.jar$S"lib/grappa1_2.jar"$S"lib/batik.jar"$S"secondoInterface/SecondoInterface.jar$S$JAVA3DPATH"

if [ -n "$SECONDO_JAVA" ]; then
  JAVA=$SECONDO_JAVA
  CLASSPATH=$SECONDO_JAVART
else 
  JAVA="java"
fi


#echo classpath= $CP


LIBPATH=lib$S$JAVA3DLIB


if [ -n "$SECONDO_GUI_HEAPSPACE" ]; then
  M=M
  HEAP=-Xmx$SECONDO_GUI_HEAPSPACE$M 
else 
  HEAP=-Xmx1024M
fi


cmd="$JAVA  $HEAP -Djava.library.path=$LIBPATH  $FEF -classpath $CP gui.MainWindow $* "

# echo $cmd


$cmd

